    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "immutable": "^4.0.0-rc.12",
    "lint-staged": "^10.2.9",
    "moment": "^2.27.0",
    "poetic": "^1.5.0",
    "prop-types": "^15.7.2",
